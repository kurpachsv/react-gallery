{
  "name": "@kurpachsv/react-gallery",
  "version": "0.0.6",
  "description": "A simple, fast and flexibility images gallery.",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint -c ./.eslintrc.json ./src",
    "eslint-check": "./node_modules/.bin/eslint --print-config ./.eslintrc.json",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "prepublish": "rm -rf ./dist && npm run build"
  },
  "author": "Sergei Kurpach <kurpachsv@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kurpachsv/react-gallery",
  "repository": {
    "type": "git",
    "url": "https://github.com/kurpachsv/react-gallery.git"
  },
  "bugs": {
    "url": "https://github.com/kurpachsv/react-gallery/issues"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.0-alpha.1",
    "@storybook/addon-links": "^4.0.0-alpha.1",
    "@storybook/addons": "^4.0.0-alpha.1",
    "@storybook/react": "^4.0.0-alpha.1",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
